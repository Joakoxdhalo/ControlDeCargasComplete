				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "TimeNow",
					"value": {
						"value": "@{formatDateTime(subtractFromTime(utcNow(), 300, 'Minute'), 'yyyyMMddHHmmss')}",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ForEach-CC",
				"type": "ForEach",
